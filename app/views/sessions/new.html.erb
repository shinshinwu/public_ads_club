<div class="container">
  <%= form_for(:session, url: {action: "create"}, html: {class: "form form-inline"}) do |f| %>
    <legend>Enter Club</legend>

    <div class="form-group">
      <%= f.email_field :email, required: true, class: "form-control", placeholder: "Email" %>
    </div>

    <div class="form-group">
      <%= f.password_field :password, required: true, class: "form-control", placeholder: "Password" %>
    </div>

    <%= f.submit "Log In", :class => 'btn btn-warning' %>

    <div class="form-group">
      <span>&nbsp;&nbsp;&nbsp;&nbsp;New user? <%= link_to "Sign up now!", signup_users_path %></span>
    </div>
  <% end %>
</div>