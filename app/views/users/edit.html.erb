<%= form_for @user, url: edit_users_path, method: :put, html: {class: "form"} do |f| %>
  <fieldset>

  <legend>Edit Profile</legend>

  <%= f.label :email %>
  <%= f.email_field :email %>

  <%= f.label :first_name %>
  <%= f.text_field :first_name %>

  <%= f.label :last_name %>
  <%= f.text_field :last_name %>

  <%= f.label :phone %>
  <%= f.telephone_field :phone %>

  <%= f.submit "Update Profile", :class => 'button' %>
  </fieldset>
<% end %>